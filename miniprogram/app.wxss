/**app.wxss**/
/* 现在npm已安装，可以取消注释 */
@import './miniprogram_npm/tdesign-miniprogram/common/style/index.wxss';

/* 覆盖TDesign图标字体配置，解决网络请求错误 */
@font-face {
  font-family: 't';
  font-style: normal;
  font-display: swap;
  font-weight: normal;
  /* 使用base64编码或者本地字体文件代替网络请求 */
  src: url('data:application/font-woff;charset=utf-8;base64, ') format('woff');
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}